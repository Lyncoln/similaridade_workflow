<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:net.sf.taverna:wfDefinition:12dba187-99ad-4990-91ff-4708c7c2cb1f" author="Alan R Williams" title="Warn - title parameter">The warn service displays a warning dialog with 'Some title' but no message.  When the user presses OK the string 'answer' is passed to the answer port of the warn service and so to the output of the workflow.</s:workflowdescription>
  <s:processor name="Warn">
    <s:defaults>
      <s:default name="title">Some title</s:default>
    </s:defaults>
    <s:local>net.sourceforge.taverna.scuflworkers.ui.WarnWorker</s:local>
  </s:processor>
  <s:link source="Warn:answer" sink="out" />
  <s:sink name="out" />
</s:scufl>

