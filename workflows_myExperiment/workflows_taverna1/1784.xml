<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:net.sf.taverna:wfDefinition:5af6daaf-6745-4a26-a914-d93d7a8e8216" author="Mikołaj Rybiński" title="sbml2Prism Web service">For example of the SBML to PRISM translation and the description of its details see http://www.prismmodelchecker.org/manual/RunningPRISM/SupportForSBML .</s:workflowdescription>
  <s:processor name="sbml2Prism">
    <s:arbitrarywsdl>
      <s:wsdl>http://bioputer.mimuw.edu.pl/tav4sb/services/wsdl</s:wsdl>
      <s:operation>sbml2Prism</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="sbml2PrismResultXML">
    <s:local>
      org.embl.ebi.escience.scuflworkers.java.XMLOutputSplitter
      <s:extensions>
        <s:complextype optional="false" unbounded="false" typename="sbml2PrismResult" name="sbml2PrismResult" qname="{http://bioputer.mimuw.edu.pl/tav4sb/services/xsd/prism}sbml2PrismResult">
          <s:elements>
            <s:basetype optional="false" unbounded="false" typename="string" name="prismModel" qname="&gt;sbml2PrismResult&gt;prismModel" />
            <s:arraytype optional="true" unbounded="true" wrapped="true" typename="string" name="warnings" qname="&gt;sbml2PrismResult&gt;warnings">
              <s:elementtype>
                <s:basetype optional="false" unbounded="false" typename="string" name="" qname="{http://www.w3.org/2001/XMLSchema}string" />
              </s:elementtype>
            </s:arraytype>
            <s:basetype optional="true" unbounded="false" typename="string" name="error" qname="&gt;sbml2PrismResult&gt;error" />
          </s:elements>
        </s:complextype>
      </s:extensions>
    </s:local>
  </s:processor>
  <s:processor name="sbml2PrismParamsXML">
    <s:description>Note: warnings 'arraytype' uses the '@wrapped=true' (warnings is an element of type 'WarningsList' which is an actual list/array of warning elements), which I'm not sure if is possible to be set via Taverna interface.</s:description>
    <s:local>
      org.embl.ebi.escience.scuflworkers.java.XMLInputSplitter
      <s:extensions>
        <s:complextype optional="false" unbounded="false" typename="sbml2PrismParams" name="sbml2PrismParams" qname="{http://bioputer.mimuw.edu.pl/tav4sb/services/xsd/prism}sbml2PrismParams">
          <s:elements>
            <s:basetype optional="false" unbounded="false" typename="string" name="sbmlModel" qname="&gt;sbml2PrismParams&gt;sbmlModel" />
            <s:basetype optional="true" unbounded="false" typename="integer" name="maxAmount" qname="&gt;sbml2PrismParams&gt;maxAmount" />
          </s:elements>
        </s:complextype>
      </s:extensions>
    </s:local>
  </s:processor>
  <s:link source="maxAmount" sink="sbml2PrismParamsXML:maxAmount" />
  <s:link source="sbml2Prism:sbml2PrismResult" sink="sbml2PrismResultXML:input" />
  <s:link source="sbml2PrismParamsXML:output" sink="sbml2Prism:sbml2PrismParams" />
  <s:link source="sbmlModel" sink="sbml2PrismParamsXML:sbmlModel" />
  <s:link source="sbml2PrismResultXML:error" sink="error" />
  <s:link source="sbml2PrismResultXML:prismModel" sink="prismModel" />
  <s:link source="sbml2PrismResultXML:warnings" sink="warnings" />
  <s:source name="sbmlModel">
    <s:metadata>
      <s:description>String with the SBML model compatible with Level 2 Version 1 of the SBML specification. Attention: it has to be a 'stochastic' version of the model, i.e. with initial amounts not initial concentrations.</s:description>
    </s:metadata>
  </s:source>
  <s:source name="maxAmount">
    <s:metadata>
      <s:description>Optional upper bound for amount of each species (to ensure a finite state space). By default translator uses an upper bound of 100. ATTENTION: for that case you should provide input value with empty string.</s:description>
    </s:metadata>
  </s:source>
  <s:sink name="prismModel">
    <s:metadata>
      <s:description>String with the model encoded in the Prism language, suitable for model-checking with Prism tool.</s:description>
    </s:metadata>
  </s:sink>
  <s:sink name="warnings">
    <s:metadata>
      <s:description>List of warnings generated by the Prism translator, such as modifications of the species names due to the Prism language restrictions.</s:description>
    </s:metadata>
  </s:sink>
  <s:sink name="error">
    <s:metadata>
      <s:description>If not empty then, instead of Prism model and optional warnings, this output contains an output error of  the Prism translator.</s:description>
    </s:metadata>
  </s:sink>
</s:scufl>

