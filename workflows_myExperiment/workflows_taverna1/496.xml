<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:net.sf.taverna:wfDefinition:b8278b1e-05a9-4332-b3b7-09984f42fb99" author="Alan R Williams" title="Extract elements from a list - single element extraction">The extract_elements_from_a_list is passed the list ['a','b','c','d','e'].  The fromIndex is 2 and the toIndex is 3, so the service outputs the single element list ['c'].</s:workflowdescription>
  <s:processor name="Extract_elements_from_a_list">
    <s:defaults>
      <s:default name="fromindex">2</s:default>
      <s:default name="toindex">3</s:default>
    </s:defaults>
    <s:local>
      org.embl.ebi.escience.scuflworkers.java.SliceList
      <s:extensions>
        <s:slicelist s:depth="1" />
      </s:extensions>
    </s:local>
  </s:processor>
  <s:processor name="Split_string_into_string_list_by_regular_expression">
    <s:defaults>
      <s:default name="string">a,b,c,d,e</s:default>
    </s:defaults>
    <s:local>org.embl.ebi.escience.scuflworkers.java.SplitByRegex</s:local>
  </s:processor>
  <s:link source="Extract_elements_from_a_list:outputlist" sink="out" />
  <s:link source="Split_string_into_string_list_by_regular_expression:split" sink="Extract_elements_from_a_list:inputlist" />
  <s:sink name="out" />
</s:scufl>

