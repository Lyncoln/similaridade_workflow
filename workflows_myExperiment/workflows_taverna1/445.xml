<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:net.sf.taverna:wfDefinition:e76467e0-81f5-4732-805f-bc9f3d7da30f" author="Alan R Williams" title="Merge string list to string - colon separator">The merge string list to string service takes the list ['a','b','c'] and using the separator ':' outputs the string 'a:b:c'.</s:workflowdescription>
  <s:processor name="Split_string_into_string_list_by_regular_expression">
    <s:defaults>
      <s:default name="string">a,b,c</s:default>
    </s:defaults>
    <s:local>org.embl.ebi.escience.scuflworkers.java.SplitByRegex</s:local>
  </s:processor>
  <s:processor name="Merge_string_list_to_string">
    <s:defaults>
      <s:default name="seperator">:</s:default>
    </s:defaults>
    <s:local>org.embl.ebi.escience.scuflworkers.java.StringListMerge</s:local>
  </s:processor>
  <s:link source="Merge_string_list_to_string:concatenated" sink="out" />
  <s:link source="Split_string_into_string_list_by_regular_expression:split" sink="Merge_string_list_to_string:stringlist" />
  <s:sink name="out">
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>application/octet-stream</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
</s:scufl>

