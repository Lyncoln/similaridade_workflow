<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:net.sf.taverna:wfDefinition:e76467e0-81f5-4732-805f-bc9f3d7da30f" author="Alan R Williams" title="Test always fails - two parameters">The test always fails service generates a service failure when two parameter values are supplied.</s:workflowdescription>
  <s:processor name="TEST___always_fails">
    <s:defaults>
      <s:default name="foo">hello</s:default>
      <s:default name="bar">world</s:default>
    </s:defaults>
    <s:local>org.embl.ebi.escience.scuflworkers.java.TestAlwaysFailingProcessor</s:local>
  </s:processor>
  <s:link source="TEST___always_fails:urgle" sink="out" />
  <s:sink name="out" />
</s:scufl>

