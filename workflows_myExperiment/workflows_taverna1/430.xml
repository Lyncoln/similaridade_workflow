<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:net.sf.taverna:wfDefinition:792f948f-5948-4899-9d0f-6fdd35c85549" author="Alan R Williams" title="Get image from URL - only url specified">Retrieve the image at http://www.mygrid.org.uk/files/2008/09/dragon-workflow.png using just the url parameter</s:workflowdescription>
  <s:processor name="Get_image_from_URL">
    <s:defaults>
      <s:default name="url">http://www.mygrid.org.uk/files/2008/09/dragon-workflow.png</s:default>
    </s:defaults>
    <s:local>org.embl.ebi.escience.scuflworkers.java.WebImageFetcher</s:local>
  </s:processor>
  <s:link source="Get_image_from_URL:image" sink="out" />
  <s:sink name="out">
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
        <s:mimeType>text/html</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
</s:scufl>

