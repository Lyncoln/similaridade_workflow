 */
class CSVString
{
  String csv;

  CSVString()
  {
    csv = "";
  }

  synchronized void setCSV(String string)
  {
    csv = string;

    notifyAll();
  }

  synchronized String getCSV()
  {
    while (csv.equals(""))
    {
      try
      {
        wait();
      }
      catch (InterruptedException e)
      {
        e.printStackTrace();
      }
    }

    return csv;
  }
}

