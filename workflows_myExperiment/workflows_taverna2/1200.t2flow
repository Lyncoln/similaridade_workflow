      } catch (ClassNotFoundException e)
      {
        e.printStackTrace();
      } catch (SQLException e)
      {
        e.printStackTrace();
      }    
      //unitsStore.add("M^(-1)*s^(-1)");
      unitsStore.add(u);
    }

    else if(u.equals("-"))
    {
      continue;
    }

}

conn.close();</script>
