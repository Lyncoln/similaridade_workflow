"&lt;INFO name=\"target_obj1\" value=\""+obj1+"\"/&gt;\n" +
"&lt;INFO name=\"target_obj2\" value=\""+obj2+"\"/&gt;\n" +
"&lt;INFO name=\"max_angle\" value=\""+angle+"\"/&gt;\n" +
