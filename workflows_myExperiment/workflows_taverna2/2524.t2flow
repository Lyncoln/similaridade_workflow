  <script>import java.text.SimpleDateFormat;

int cad = Integer.parseInt(cadence) * 60 *60 * 1000;

end_dates=new ArrayList();
start_dates= new ArrayList();

if(dates.size()==0){
  return;	
}


String tz="+0000";

SimpleDateFormat sdf = new SimpleDateFormat("yyyy-MM-dd'T'HH:mm:ssZ");

int first=-1;
long date1;
long date2;
 
for(int i=0; i&lt;dates.size(); i++){
  if(first==-1){
    first = i;
    date1 = sdf.parse(dates.get(i)+tz).getTime();
    start_dates.add(dates.get(first));
  } else {
    date2 = sdf.parse(dates.get(i)+tz).getTime();
    if(date1 + cad &lt; date2) {
      i--;
      end_dates.add(dates.get(i));
      first = -1;
    } else {
      date1 = date2;    	
    }

  } 	
}
end_dates.add(dates.get(dates.size()-1));

</script>
