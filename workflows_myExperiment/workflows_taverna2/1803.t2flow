  <script>String newList = "";
Map temp = new HashMap();
for(Object csv:listOfCsv){
    String csv_ = (String)csv;
    String[] list = csv_.split(",");
    for(String acc:list){
        if(!temp.containsKey(acc)){
            temp.put(acc,1);
        } else {
            Integer count = Integer.parseInt(temp.get(acc).toString());
            count++;
            temp.put(acc, count.toString());
        }
    }
}

Integer thresholdValue = Integer.parseInt(threshold);
for(Object acc:temp.keySet()){
    Integer count = Integer.parseInt(temp.get(acc).toString());
    if(count &gt;= thresholdValue){
        if(newList.length() &gt; 0){
            newList += ",";
        }
        newList += acc;
    }
}</script>
