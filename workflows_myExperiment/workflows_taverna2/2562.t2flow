  <script>import java.util.ArrayList;

ArrayList trigger_list = new ArrayList();

int start_value = Integer.parseInt(start);
int end_value = Integer.parseInt(end);
int step_value = Integer.parseInt(step);

if (step_value &lt;= 0) {
	throw new RuntimeException("The step must be greater than zero");
}

for (int i = start_value; i &lt;= end_value; i += step_value) {
	trigger_list.add(i);	
}
</script>
