  <script>
URL inputURL = null;

String query = "SELECT+DISTINCT+%3Fs+WHERE+{%3Fs+%3Fp+%3Fo+.+%3Fo+bif%3Acontains+'%22"+term+"%22'+.+FILTER(+regex(%3Fs%2C+%22"+namespace+"%3A%22))+.}%20LIMIT%20" + limit;
inputURL = new URL(sparql_endpoint + "?default-graph-uri=&amp;query="+query+"&amp;format=application/sparql-results+xml&amp;debug=on");

URLConnection con = inputURL.openConnection();
InputStream in = con.getInputStream();

InputStreamReader isr = new InputStreamReader(in);
Reader inReader = new BufferedReader(isr);

StringBuffer buf = new StringBuffer();
int ch;
while ((ch = inReader.read()) &gt; -1) {
  buf.append((char)ch);
}
inReader.close();
sparql_xml = buf.toString();
</script>
