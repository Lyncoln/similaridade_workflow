  <script>String n3;
URL inputURL = null;

//inputURL = new URL(sparql_endpoint + "?default-graph-uri=&amp;query=DESCRIBE+&lt;http%3A%2F%2Fbio2rdf.org%2F" + bmuri + "&gt;&amp;format=text%2Frdf%2Bn3&amp;debug=on");
