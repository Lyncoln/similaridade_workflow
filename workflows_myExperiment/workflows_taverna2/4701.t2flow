  <script>/*
	Inputs: 	meta_country_list      (1)
				meta_count_list        (1)
				former_countries_list  (1)
				current_countries_list (1)
				
	Outputs:	country_list (1)
				count_list   (1)
*/

	country_list = new ArrayList();
	count_list = new ArrayList();
	successor_countries = new ArrayList();
	
	//creates hashtable with country codes as keys and counts as values 
	country_count = new Hashtable();
	
	for(int i=0; i&lt;meta_country_list.size(); i++){
         country_count.put(meta_country_list.get(i),meta_count_list.get(i));
    }
	
	//loops for each former country
	int i = 0;
	while(i&lt;former_countries_list.size()){
		//get the former country
		fc = former_countries_list.get(i);
		//get its first successor country
		successor_countries.add(current_countries_list.get(i));
		i++;
		//loop, if the former country has more entries (more successor countries) and add them to successor_countries list
		while(former_countries_list.get(i).equals(fc)){
			successor_countries.add(current_countries_list.get(i));
			i++;
			if(i==former_countries_list.size())
				break;
		}
		//get the count for the former country
		temp_fc_count = country_count.get(fc);
		//loop for each successor of the former country
		for(int c=0; c&lt;successor_countries.size(); c++){
			//get the already existing count for the successor country and add the count of the former country
			temp_successor_count = Integer.parseInt(country_count.get(successor_countries.get(c))) + Integer.parseInt(temp_fc_count);
			//update the count for the successor country with the added value from the former country
			country_count.put(successor_countries.get(c), temp_successor_count);
		}
		//clean the successor_countries list for the next former country
		successor_countries.clear();
	}

//copy the same country list	
for(int i=0; i&lt;meta_country_list.size(); i++){
	country_list.add(meta_country_list.get(i));
}	

//copy the updated count list
for(int i=0; i&lt;country_list.size(); i++){
	count_list.add(country_count.get(country_list.get(i)));
}
	</script>
