  <script>String stupid = unique_identifiers;

String[] blastline = stupid.split("\n");
Set giSet = new HashSet();
String placeholder = "";
for (int i=0;i&lt;blastline.length;i++)
{
	String[] line = blastline[i].split("ref");
	String zero = line[1];
	String gi = "";
	String pattern = "[A-Z]+\\d+([.]\\d)?";
	java.util.regex.Matcher matcher = java.util.regex.Pattern.compile(pattern).matcher(zero);
	if(matcher.find())
	{	
		gi = zero.substring(matcher.start(), matcher.end());
		giSet.add(gi);	
	}

	

}

for (Object o: giSet)
{
	placeholder += o.toString() + "\n";
}

out1 = placeholder;
</script>
