lines = Tid.split("\n");
output = "&lt;p&gt;&lt;b&gt;&lt;u&gt;Target species:&lt;/u&gt;&lt;/b&gt;&lt;/p&gt;";
for (String line: lines)
{
	output+= "&lt;p&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;"+line+"&lt;/p&gt;";
}

out.write(output);



//Source Proteome IDs:
//Beanshell script:



lines = Sid.split("\n");
output = "&lt;p&gt;&lt;b&gt;&lt;u&gt;Source species:&lt;/u&gt;&lt;/b&gt;&lt;/p&gt;";
for (String line: lines)
