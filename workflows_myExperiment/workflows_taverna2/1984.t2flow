}

out.write(output);



//Similarity Measure:
//Beanshell script:



lines = Sim_M.split("\n");
 output = "";
for (String line: lines)
{
	output+= "&lt;p&gt;"+line+"&lt;/p&gt;";
}

out.write(output);


//Total List of Identified Proteins:
//Beanshell script:



lines = TotalID_d.split("\n");
output = "&lt;p&gt;&lt;b&gt;&lt;u&gt;Identified proteins:&lt;/u&gt;&lt;/b&gt;&lt;/p&gt;";
for (String line: lines)
{
	output+= "&lt;p&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;"+line.split("\t")[0]+"&lt;/p&gt;";
}

out.write(output);
//////////////////////////////////////////////////////////////////
out.close();
outputFile = output;
