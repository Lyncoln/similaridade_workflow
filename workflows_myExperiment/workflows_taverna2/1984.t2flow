  <script>String [] blast_results = blast_file.split("\n");
String result = "";
double thresIdent = (new Double(thresholdIdentity)).doubleValue();

for (int i=0; i&lt;blast_results.length; i++)
{
	String[] line = blast_results[i].split("\t");
//Query id, 0
//Subject id, 1
//percent of identity, 2
//alignment length, 3
//number of mismatches (not including gaps), 4
//number of gap openings, 5
//start of alignment in query,6
//end of alignment in query, 7
//start of alignment in subject,8
//end of alignment in subject, 9
//expected value, 10
//bit score. 11
	double identity = (new Double(line[2])).doubleValue();
	if (identity &gt;= thresIdent)
	{
		result += blast_results[i]+"\n";
	} 
	
}

out1 = result.trim();</script>
