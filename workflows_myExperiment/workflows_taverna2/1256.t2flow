  <script>Hashtable organismAccession = new Hashtable();

for (int i = 0; i &lt; organismNameResult.size(); i++) {
	String[] organismLines = organismNameResult.get(i).split("\n");
	String organism = organismLines[2];
	if (!organismAccession.containsKey(organism)) {
		organismAccession.put(organism, accessionAndSimilarity.get(i).get(0) + "\t" + accessionAndSimilarity.get(i).get(1));
	}
}

String[] keys = new String[organismAccession.size()];
Enumeration enu = organismAccession.keys();
int count = 0;
while (enu.hasMoreElements()) {
	keys[count] = (String) enu.nextElement();
	count++;
}
Arrays.sort(keys);
StringBuffer sb = new StringBuffer();
sb.append("DDBJ entries:\t" + arsaResultNum + "\n");
sb.append("Representative accession:\t" + representiveAccession + "\n");
sb.append("Organism name\tDDBJ accession number\tSequence similarity \n");
for (int i = 0; i &lt; keys.length; i++) {
	sb.append(keys[i] + "\t" + (String)organismAccession.get(keys[i]) + "\n");
}
result = sb.toString();</script>
