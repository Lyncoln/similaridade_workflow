  <script>import java.util.regex.*;

Pattern thePat1 = Pattern.compile("&lt;entry_id xsi:type=.xsd:string.&gt;(.*?)&lt;/entry_id&gt;");
Pattern thePat2 = Pattern.compile("&lt;definition xsi:type=.xsd:string.&gt;(.*?)&lt;/definition&gt;");

String namespace, desc, uri, nsid, ntriples;

ntriples = "";

	Matcher matcher1 = thePat1.matcher(string);
	if (matcher1.find()) {
		namespace = matcher1.group(1);
		
		uri = "&lt;http://bio2rdf.org/ns/" + namespace + "&gt;";
		nsid = "bm:ns:" + namespace;
		ntriples = ntriples + "\n\n" + uri + " &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#type&gt; &lt;http://bio2rdf.org/ns/bio2rdf#Namespace&gt; .";
		ntriples = ntriples + "\n" + uri + " &lt;http://bio2rdf.org/ns/bio2rdf#xSource&gt; &lt;http://bio2rdf.org/ns/kegg&gt; .";
		ntriples = ntriples + "\n" + uri + " &lt;http://purl.org/dc/elements/1.1/identifier&gt; \"" + nsid + "\" .";
		ntriples = ntriples + "\n" + uri + " &lt;http://bio2rdf.org/ns/bio2rdf#url&gt; \"http://www.genome.jp/dbget-bin/www_bfind_sub?mode=bfind&amp;max_hit=1000&amp;dbkey=" + namespace +"\" .";
		ntriples = ntriples + "\n" + uri + " &lt;http://bio2rdf.org/ns/bio2rdf#namespace&gt; \"" + namespace +"\" .";
	}

	Matcher matcher2 = thePat2.matcher(string);
	if (matcher2.find()) {
		desc = matcher2.group(1);
		ntriples = ntriples + "\n" + uri + " &lt;http://purl.org/dc/elements/1.1/title&gt; \"" + desc + "\" .";
		ntriples = ntriples + "\n" + uri + " &lt;http://www.w3.org/2000/01/rdf-schema#label&gt; \"" + desc + " [" + nsid + "]\" .";
	
	}
	

</script>
