colnames(mw)[1]&lt;-"MassOfWaters";
ms&lt;-specimenData["FinalContDrySpecMass"] - specimenData["ContMass"]; 
colnames(ms)[1]&lt;-"MassOfSolids";
w&lt;-(mw/ms)*100;
colnames(w)[1]&lt;-"WaterContent";
totalData&lt;-cbind(specimenData,mw,ms,w);
