				{
					s=getSequence.next();
				}	
				else{
				out1 = out1.concat(s);				
s3 = s.getBytes();

				out.write(s3);
				s=getSequence.next();
				}
			}

			if (s.charAt(0)!='&gt;')
			{
				out1 = out1.concat(s);	
			s3 = s.getBytes();
			out.write(s3);
			}
			
