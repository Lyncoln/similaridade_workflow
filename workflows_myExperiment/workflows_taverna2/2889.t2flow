	// create a new string for storing the sequence 
 	out1 = new String();
	// create a file output stream from the provided output file location 
			FileOutputStream out = null;
            out = new FileOutputStream(new File(SeqFileURL));
			getSequence =new Scanner(new File(FastaFileURL));
	// temporary virable
			byte[] s3={};
  			String s = getSequence.next();
	// concat all the information into a whole string	
		while (getSequence.hasNext())
			{
