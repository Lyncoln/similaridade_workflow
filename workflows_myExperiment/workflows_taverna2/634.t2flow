  <script>URL inputURL = null;

inputURL = new URL("http://localhost:8890/sparql?default-graph-uri=&amp;should-sponge=soft&amp;query=SELECT+%3Fs%2C+%3Fo+FROM+&lt;http%3A%2F%2Fbio2rdf.org%2F"+uri+"&gt;+WHERE+{%3Fs+&lt;http%3A%2F%2Fwww.w3.org%2F1999%2F02%2F22-rdf-syntax-ns%23type&gt;+%3Fo}&amp;format=text%2Fhtml&amp;debug=on");
