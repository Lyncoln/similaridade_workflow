	if(l.indexOf("&lt;LinkName&gt;snp_pubmed_cited&lt;/LinkName&gt;") &gt; -1) {
		target = true;
		continue;
	}
	if(target) {
		if(l.indexOf("&lt;Id&gt;") &gt; -1) {
			l = l.substring(l.indexOf("&lt;Id&gt;") + 4);
			l = l.substring(0, l.indexOf("&lt;"));
			pubmedIdList.add(l);
		} else if(l.indexOf("&lt;/LinkSetDb&gt;") &gt; -1) {
			break;
		}
	}
}
br.close();</script>
