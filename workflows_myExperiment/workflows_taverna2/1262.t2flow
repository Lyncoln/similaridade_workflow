  <script>String entry = "";

for(int i=0; i&lt;entryList.size(); i++){
	String str = (String)entryList.get(i);
	int start = Integer.parseInt((String)startList.get(i));
	int end = Integer.parseInt((String)endList.get(i));

	BufferedReader br = new BufferedReader(new StringReader(str));
	StringBuffer dnaBuf = new StringBuffer();
	String accession = "";
	String org = "";
	String dna = "";
	String line;
	while( (line=br.readLine()) != null){
		if(line.startsWith("ACCESSION")){
			accession = line.substring(12, line.length());
		}
		if(line.startsWith("  ORGANISM")){
			String[] sa = line.substring(12, line.length()).split(" ");
			org = Character.toString(sa[0].charAt(0));
			for(int j=1; j&lt;sa.length; j++){
				org += "_" + sa[j];
			}
		}
		if(line.startsWith("ORIGIN")){
			while(!(line=br.readLine()).equals("//")){
				String[] sa = line.trim().split(" ");
				for(int j=1; j&lt;sa.length; j++){
					dnaBuf.append(sa[j]);
				}
			}
			break;
		}
	}
	if(start &gt; end){
		dna = dnaBuf.subSequence(end, start);
	}else{
		dna = dnaBuf.subSequence(start, end);
	}
	entry += "&gt;" + accession + "|" + org + "\n" + dna + "\n";
}</script>
