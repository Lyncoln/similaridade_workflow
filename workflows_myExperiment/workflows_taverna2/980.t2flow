  <script>import java.util.regex.*;

String query = search;
search = search.replace(" ","_");

bmuris = new ArrayList();
ntriples = new ArrayList();
String bmuri;
Integer count = 0;

Pattern thePat = Pattern.compile("^(.*?) ");

for (Iterator i = stringlist.iterator(); i.hasNext();) {
	String item = (String) i.next();
	Matcher matcher = thePat.matcher(item);

	if (matcher.find()) {
		bmuri = "http://bio2rdf.org/"+matcher.group(1);
		count ++;
		bmuris.add(bmuri);
	}
}

count_ntriple = "";

if (count != 0) {
	String bmuri = "http://bio2rdf.org/searchns/kegg_" + namespace + "/" + search;
	count_ntriple =                 "&lt;" + bmuri + "&gt; &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#type&gt; &lt;http://bio2rdf.org/ns/bio2rdf#SearchResults&gt; .\n";
	count_ntriple = count_ntriple + "&lt;" + bmuri + "&gt; &lt;http://bio2rdf.org/ns/search#query&gt; \"" + query + "\" .\n";
	count_ntriple = count_ntriple + "&lt;" + bmuri + "&gt; &lt;http://bio2rdf.org/ns/search#resultCount&gt; \"" + count + "\" .\n";
	count_ntriple = count_ntriple + "&lt;" + bmuri + "&gt; &lt;http://bio2rdf.org/ns/search#xProvider&gt; &lt;http://bio2rdf.org/ns/kegg&gt; .\n";
	count_ntriple = count_ntriple + "&lt;" + bmuri + "&gt; &lt;http://bio2rdf.org/ns/search#xNamespace&gt; &lt;http://bio2rdf.org/ns/" + namespace + "&gt; .\n";
}
</script>
