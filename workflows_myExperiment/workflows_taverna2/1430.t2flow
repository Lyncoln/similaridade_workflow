  <script>
if(choice!="Skip")
{
String cmd = ctf+"MRIViewer";

Process proc = null;
Runtime rt = Runtime.getRuntime();

proc = rt.exec(cmd);

// Get the input stream and read from it
InputStream in = proc.getInputStream();

int c;
StringBuffer sb = new StringBuffer();
while ((c = in.read()) != -1) {
	sb.append((char) c);
}
in.close();
result = sb.toString();

}
  
</script>
