  <script>// parse protocol file
// look for csv bands and piped triggers
String[] pFileLines = protocol.split("\\n");
String[] bands;

ArrayList windows = new ArrayList();
ArrayList windowFiles = new ArrayList();

for(int i=0;i&lt;pFileLines.length;i++)
{
	if(!pFileLines[i].startsWith("//") &amp;&amp; pFileLines[i].length()&gt;1)
	{
		if(pFileLines[i].indexOf(",")&gt;0)
		{ // freq bands
			bands = pFileLines[i].split(";");
		}

		if(pFileLines[i].indexOf("|")&gt;0)
		{ // time window
			 windows.add(pFileLines[i]);
		}
	}	
}

String out0;

// create file for each window
for(int x=0;x&lt;windows.size();x++)
{
	String temp = windows.get(x);

	String[] parts =temp.split("\\|");
	parts[0] = parts[0].trim();
	parts[0] = parts[0].replace(" ","\t");
	parts[1] = parts[1].trim();
	parts[1] = parts[1].replace(" ","\t");

	temp = temp.replace(" | ","-");
	temp = temp.replace(" ","_");

	BufferedWriter out = new BufferedWriter(new FileWriter(dataset+"/SAM/"+temp, false));
	out.write("1\n"+parts[0]+"\n1\n"+parts[1]);
	out.close();

	 windowFiles.add(temp);
}

Process proc = null;
Runtime rt = Runtime.getRuntime();

// generate commands
for(y=0;y&lt;bands.length;y++)
{
	String[] bandSplit = bands[y].split(",");
	for(z=0;z&lt;windowFiles.size();z++)
	{
		String cmd = "qsub -cwd  -j y  -S /bin/bash -V "+SAMwrap+" -C -r "+dataset+" -m "+windowFiles.get(z)+" -f "+bandSplit[0]+" "+bandSplit[1]+" -x -7 14 -y -9 9 -z 0 14 -s 0.5 -l 1 -D2 -p -n -v";
		proc = rt.exec(cmd);
	}
}

