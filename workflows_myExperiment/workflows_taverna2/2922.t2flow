  <script>import java.text.SimpleDateFormat;

dateOut = new ArrayList();

SimpleDateFormat sdf = new SimpleDateFormat("yyyy-MM-dd'T'HH:mm:ss");
SimpleDateFormat sdf2 =  new SimpleDateFormat("yyyy/MM/dd/");


Date d = sdf.parse(date);
long l = d.getTime();
long l2 = sdf.parse(date2).getTime();
long day= 20*60*60*1000;

dateOut.add(sdf2.format(d));

while(l+day&lt;=l2){
  l = l + day;
  Date d2= new Date(l);
  if(sdf2.format(d).compareTo(sdf2.format(d2))!=0){
     dateOut.add(sdf2.format(d2));
     d=d2;
  }
}
Date d2= new Date(l2);
if(sdf2.format(d).compareTo(sdf2.format(d2))!=0){
   dateOut.add(sdf2.format(d2));
}

</script>
