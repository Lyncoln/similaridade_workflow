  <script>String [] blast_results = blast_file.split("\n");
double total = 0;

//make a map of query proteins to their highest identity
HashMap map = new HashMap();

for (int i=0; i&lt;blast_results.length; i++)
{
	String[] line = blast_results[i].split("\t");
	//query is in position 1
	String source = line[1].trim();

	//get percent identity, index 2
	double newScore = (new Double(line[2]));

	//get the source protein if existing
	Object o = map.get(source);
	if (o != null)
	{
		//compare the score in the map with the current score
		//and take the higher one
		double scoreSoFar = (new Double(o.toString()));
		if (newScore &gt; scoreSoFar)
		{
			map.put(source, newScore);
		}
	}
	else //if the source protein is not in the map yet, add it and this value
	{
		map.put(source, newScore);
	}	
}

//sum all scores
for (Object value: map.values())
{
	total += (new Double(value.toString()));
}

//output the average
out1 = total/map.size();</script>
