  <script>String in = unique_identifiers;

//split input file into lines
String[] blastline = in.split("\n");

//make necessary variables
Set giSet = new HashSet();
String placeholder = "";

//for each line
for (int i=0;i&lt;blastline.length;i++)
{
	//split it at "ref"
	String[] line = blastline[i].split("ref");

	//pick the second index (after the "ref")
	String zero = line[1];
	String gi = "";

	//find the EMBL id pattern
	String pattern = "[A-Z]+\\d+([.]\\d)?";
	java.util.regex.Matcher matcher = java.util.regex.Pattern.compile(pattern).matcher(zero);
	//find the next match and save it
	if(matcher.find())
	{	
		gi = zero.substring(matcher.start(), matcher.end());
		giSet.add(gi);	
	}
}

//add all elements of the set to an output string
for (Object o: giSet)
{
	placeholder += o.toString() + "\n";
}

out1 = placeholder;
</script>
