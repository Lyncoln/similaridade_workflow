  <script>//import tab-delimited blast results
	
	//split the input on new lines
	String[] input = xml_result.split(System.getProperty("line.separator"));
        ArrayList gi_lines = new ArrayList();

	//for each line in the BLAST file
	for (int i=0; i&lt;input.length; i++)
	{
		//split the line into columns
		String[] line = input[i].split("\\s");
		//get the target id from the 1st column
		String targetId = line[0].trim();
		

		//if we haven't already stored this ID, add it
		if (!gi_lines.contains(targetId))
		{
			gi_lines.add(targetId);
		}
	}	

	//Output the gi ids as a string
        String gi_val_out = "";
        for(int i = 0; i&lt;gi_lines.size(); i++) {
            gi_val_out = gi_val_out + gi_lines.get(i).toString() + "\n";
        }

//export gi ids
gi_lines_val = gi_val_out;</script>
